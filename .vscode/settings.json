{
    "breadcrumbs.enabled": false,
    "debug.internalConsoleOptions": "neverOpen",
    "dev.containers.dockerComposePath": "docker compose",
    "diffEditor.codeLens": true,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "diffEditor.diffAlgorithm": "advanced",
    "editor.colorDecorators": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.detectIndentation": false,
    "editor.emptySelectionClipboard": false,
    "editor.experimental.asyncTokenization": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.foldingImportsByDefault": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.guides.bracketPairs": true,
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.hover.enabled": "on",
    "editor.lightbulb.enabled": "off",
    "editor.matchBrackets": "never",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.occurrencesHighlight": "off",
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.scrollbar.horizontalScrollbarSize": 1,
    "editor.scrollbar.verticalScrollbarSize": 1,
    "editor.selectionHighlight": false,
    "editor.snippetSuggestions": "top",
    "editor.stickyScroll.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.excludeGitIgnore": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.blame.editorDecoration.enabled": true,
    "git.decorations.enabled": false,
    "git.enableSmartCommit": true,
    "git.supportCancellation": true,
    "git.followTagsWhenSync": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "problems.decorations.enabled": false,
    "redhat.telemetry.enabled": false,
    "rest-client.defaultHeaders": {
        "Content-Type": "application/json",
        "User-Agent": "vscode-restclient"
    },
    "rest-client.enableTelemetry": false,
    "rest-client.logLevel": "verbose",
    "rest-client.previewResponsePanelTakeFocus": false,
    "rest-client.previewOption": "exchange",
    "scm.defaultViewMode": "tree",
    "scm.diffDecorations": "none",
    "scm.graph.showIncomingChanges": true,
    "scm.graph.showOutgoingChanges": true,
    "scm.repositories.explorer": true,
    "scm.repositories.selectionMode": "multiple",
    "git.branchRandomName.enable": true,
    "git.verboseCommit": true,
    "git.useEditorAsCommitInput": true,
    "git.detectSubmodules": true,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.shellIntegration.history": 100,
    "terminal.integrated.suggest.enabled": true,
    "terminal.integrated.suggest.selectionMode": "always",
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "todohighlight.isCaseSensitive": false,
    "todohighlight.keywords": [
        "HACK:",
        "DEBUG:",
        "REVIEW:",
        "NOTE:",
        "HINT:"
    ],
    "todohighlight.defaultStyle": {
        "backgroundColor": "transparent"
    },
    "workbench.activityBar.location": "bottom",
    "workbench.colorTheme": "Tokyo Night Storm",
    "workbench.editor.enablePreview": false,
    "workbench.editor.showTabs": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "window.nativeFullScreen": true,
    "window.newWindowProfile": "Default",
    "window.newWindowDimensions": "inherit",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.format.enable": true,
    "chat.agent.enabled": true,
    "chat.mcp.assisted.nuget.enabled": true,
    "chat.mcp.autostart": "newAndOutdated",
    "chat.mcp.gallery.enabled": true,
    "editor.fontFamily": "'JetBrainsMono Nerd Font', Monaco, 'Courier New', monospace",
    "editor.aiStats.enabled": true,
    "editor.inlineSuggest.enabled": false,
    "github.copilot.chat.agent.runTasks": true,
    "github.copilot.chat.codeGeneration.useInstructionFiles": true,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.nextEditSuggestions.fixes": true,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "github.copilot.chat.enableRetryAfterFilteredResponse": true,
    "github.copilot.chat.newWorkspace.useContext7": true,
    "github.copilot.chat.scopeSelection": true,
    "remote.autoForwardPortsSource": "hybrid",
    "update.mode": "none",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "window.zoomPerWindow": false,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "window.zoomLevel": 1
}
